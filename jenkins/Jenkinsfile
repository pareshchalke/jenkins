node {
    checkout scm
    sh("ls")
    def URL = "http://172.17.0.1:8081/nexus"
    def p = load "lib.groovy"

    def output = p.getnexusrepo(URL)

    genlist = p.generatelist(output as String[],"repolist")
    println "Iterate genlist"
    genlist.each {
        //println it
        status = p.createrepo( it, URL)
        //groupdata = p.generategrouppostdata( it, URL )
        repoadd = p.addrepo( it , URL )
        //println groupdata
    }
    println "Iterate genlist"
}
